<div>
  <h3>Suivi de materiel</h3>

  <div class="col py-3">
    <ul class="nav nav-tabs" id="myTab" role="tablist">
      <li class="nav-item" onclick="loadAllUser();">
        <a
          class="nav-link active"
          id="home-tab"
          data-toggle="tab"
          role="tab"
          aria-controls="home"
          aria-selected="true"
        >User
        </a>
      </li>

      <li class="nav-item" onclick="loadAllMateriel();">
        <a
          class="nav-link"
          id="home-tab"
          data-toggle="tab"
          role="tab"
          aria-controls="home"
          aria-selected="true"
        >
          <i></i>Materiel
        </a>
      </li>
    </ul>
    <div class="tab-content overflow-auto tabSize" id="myTabContent">
					{% for item in icon %}
						{% if item == icon[0] %}
							{% block listeActive %}{% endblock %}
						{% else %}
							{% block liste %}{% endblock %}
						{% endif %}
					{% endfor %}
				</div>
  </div>
</div>

<div class="modal fade" id="modal" tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
		<div class="modal-dialog">
			<div class="modal-content">

				
			</div>
		</div>
</div>


